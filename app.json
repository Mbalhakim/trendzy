{
  "expo": {
    "name": "Trendzy",
    "slug": "trendzy",
    "scheme": "trendzy",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo-modified.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/SplashNoColor.png",
      "resizeMode": "contain",
      "backgroundColor": "#020617"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo-modified.png",
        "backgroundColor": "#020617",
        "package": "com.bashardev.trendzy"
      },
      "package": "com.bashardev.trendzy"
    },
    "web": {
      "favicon": "./assets/images/logo-modified-small.png"
    },
    "plugins": [
      "expo-router"
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "16753d93-ddcf-4f86-ac32-86b62c31130e"
      }
    }
  }
}
